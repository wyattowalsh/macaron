  "license_file": "LICENSE",
  "license_header": true,
  "line_length": 88,
  "quote_style": "double",
  "line_endings": "\\n",
  "gtm_container_id": "GTM-P9MQGWXJ",
  "use_google_analytics": true,
  "use_plausible": false,
