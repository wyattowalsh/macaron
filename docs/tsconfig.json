{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
      "baseUrl": ".",
      "paths": {
          "@/*": ["./src/*"],
          "@/lib/*": ["./src/lib/*"],
          "@/components/*": ["./src/components/*"],
          "@/hooks/*": ["./src/hooks/*"]
      }
  },
  "include": ["./src"],
  "exclude": ["./node_modules"]
}
